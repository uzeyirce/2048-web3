{
  "rewrites": [
    {
      "source": "/.well-known/(.*)",
      "destination": "/.well-known/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/index.html" //!!! BURAYI GÜNCELLEDİK !!!
    }
  ],
  "headers": [
    {
      "source": "/.well-known/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=600" }
      ]
    }
  ],
  "cleanUrls": true
}
